<section id="contact">
    <div class="container">
        <div class="row">
            <div class="col-lg-8 col-lg-offset-2 text-center">
                <h2 class="section-heading">{{ metadata.contact.title }}</h2>
                <hr class="primary">
                <p>{{ metadata.contact.description }}</p>
            </div>
        <div class="col-lg-2 col-lg-offset-2 text-center">
            <a href="https://t.me/foodrescuesengkang">
                <i class="fab fa-telegram-plane fa-5x wow bounceIn" data-wow-delay=".1s"></i>
                <p>{{ metadata.contact.cta1 }}</p>
            </a>
        </div>
        <div class="col-lg-2 col-lg-offset-1 text-center">
            <a href="mailto:foodrescuesk@gmail.com">
            <i class="fas fa-envelope fa-5x wow bounceIn" data-wow-delay=".1s"></i>
                <p>{{ metadata.contact.cta2 }}</p>
            </a>
        </div>
        <div class="col-lg-2 col-lg-offset-1 text-center">
            <a href="https://www.facebook.com/foodrescuesk/">
            <i class="fas fa-bullhorn fa-5x wow bounceIn" data-wow-delay=".1s"></i>
                <p>{{ metadata.contact.cta3 }}</p>
            </a>
        </div>

    </div>
</section>
{% comment %} <footer class="footer">
      <div class="container">
        <span class="text-muted">Place sticky footer content here.</span>
      </div>
    </footer> {% endcomment %}
